{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.example.com/api/:path*"
    },
    {
      "source": "/media/:path*",
      "destination": "https://api.example.com/media/:path*"
    },
    {
      "source": "/docs/api",
      "destination": "https://api.example.com/docs/api/"
    },
    {
      "source": "/ws/:path*",
      "destination": "https://api.example.com/ws/:path*"
    }
  ]
}
